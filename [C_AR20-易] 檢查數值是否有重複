#include<iostream>  
using namespace std;  
  
  
  
int main(void){      
      
    int a = 0;  
    int num[150];  
    int visit[150] = {0};  
    int b = 1;  
    cin >> a;  
    for (int i = 0; i < a; i++){  
        cin >> num[i];  
        visit[num[i]]++;  
        if (visit[num[i]] > 1) {  
            cout << "0\n";  
            b = 0;  
        }  
    }  
  
    if (b){  
        cout << "1\n";  
    }  
  
  
  
    return 0;      
}
